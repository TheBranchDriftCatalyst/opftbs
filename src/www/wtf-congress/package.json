{
  "name": "@examples/next-websockets-starter",
  "version": "10.38.0",
  "private": true,
  "engines": {
    "node": "^18.15.0"
  },
  "scripts": {
    "changelog": "conventional-changelog -i CHANGELOG.md -s -r 0",
    "commit": "cz",
    "prebuild": "prisma generate && prisma migrate deploy",
    "build:1-next": "cross-env NODE_ENV=production next build",
    "build:2-server": "tsc --project tsconfig.server.json",
    "build": "run-s build:*",
    "dev:wss": "cross-env PORT=3001 tsx watch src/server/wssDevServer.ts --tsconfig tsconfig.server.json ",
    "dev:next": "next dev",
    "dev": "pnpm migrate-dev && bash -c 'run-p dev:*'",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dx": "run-s db-up db-migrate-dev db-seed dev",
    "dev-nuke": "rm -rf prisma/*.db**",
    "generate": "prisma generate",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "start": "cross-env NODE_ENV=production node dist/server/prodServer.js",
    "studio": "prisma studio",
    "lint": "eslint --cache --ext \".js,.ts,.tsx\" --report-unused-disable-directives --report-unused-disable-directives src",
    "lint-fix": "pnpm lint --fix",
    "test-dev": "cross-env APP_ENV=test start-server-and-test dev 3000 test",
    "test-start": "cross-env APP_ENV=test WS_URL=ws://localhost:3000 start-server-and-test start 3000 test",
    "test": "playwright test",
    "postinstall": "prisma generate"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.9",
    "@mui/material": "^5.14.7",
    "@mui/x-data-grid": "^6.14.0",
    "@prisma/client": "^5.2.0",
    "@tanstack/react-query": "^4.18.0",
    "@trpc/client": "^10.38.0",
    "@trpc/next": "^10.38.0",
    "@trpc/react-query": "^10.38.0",
    "@trpc/server": "^10.38.0",
    "@types/bcrypt": "^5.0.0",
    "@types/lodash": "^4.14.198",
    "@types/uuid": "^9.0.4",
    "bcrypt": "^5.1.1",
    "clsx": "^2.0.0",
    "commitizen": "^4.3.0",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "lodash-contrib": "^4.1200.1",
    "next": "^13.4.8",
    "next-auth": "^4.22.1",
    "next-themes": "^0.2.1",
    "prisma-trpc-generator": "^1.4.1",
    "rar-stream": "^3.1.1",
    "react": "^18.2.0",
    "react-data-table": "^0.1.0",
    "react-dom": "^18.2.0",
    "react-swipeable": "^7.0.1",
    "remark": "^15.0.1",
    "remark-html": "^16.0.1",
    "sanitize-html": "^2.11.0",
    "superjson": "^1.12.4",
    "trpc-shield": "^0.4.3",
    "tsx": "^3.12.7",
    "util": "^0.12.5",
    "uuid": "^9.0.1",
    "ws": "^8.0.0",
    "yarn": "^1.22.19",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@mermaid-js/mermaid-cli": "^10.4.0",
    "@mui/x-data-grid-generator": "^6.14.0",
    "@playwright/test": "^1.26.1",
    "@tanstack/react-query-devtools": "^4.18.0",
    "@types/node": "^18.16.16",
    "@types/react": "^18.2.8",
    "@types/sanitize-html": "^2.9.1",
    "@types/ws": "^8.2.0",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "autoprefixer": "^10.4.7",
    "conventional-changelog-cli": "^4.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "date-fns": "^2.30.0",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.3",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "pnpm": "^8.7.1",
    "postcss": "^8.4.14",
    "prettier": "^2.8.8",
    "prisma": "5.3.1",
    "prisma-docs-generator": "^0.8.0",
    "prisma-erd-generator": "^1.11.1",
    "start-server-and-test": "^1.12.0",
    "tailwindcss": "^3.3.0",
    "typescript": "^5.1.3",
    "unrar": "^0.2.0"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
